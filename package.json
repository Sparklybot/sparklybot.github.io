    "gulp": "^4.0.2",
    "gulp-csso": "^4.0.1",
    "gulp-postcss": "^8.0.0",
    "gulp-purgecss": "^2.2.0",
    "gulp-sass": "^4.1.0",
    "gulp-terser": "^1.2.0",
    "gulp-webp": "^4.0.1",
    "tailwindcss": "^1.4.6"
  }
}
