{
  "name": "sparkly",
  "version": "1.0.0",
  "description": "Sparkly website",
  "private": true,
  "scripts": {
    "build": "set NODE_ENV=development&&set JEKYLL_ENV=development&& gulp build",
    "build-prod": "set NODE_ENV=production&&set JEKYLL_ENV=production&& gulp build",
    "serve": "set NODE_ENV=development&&set JEKYLL_ENV=development&& gulp build && bundle exec jekyll serve"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "gulp": "^4.0.2",
    "gulp-csso": "^3.0.1",
    "gulp-postcss": "^9.0.1",
    "gulp-purgecss": "^1.2.0",
    "gulp-sass": "^4.1.1",
    "gulp-terser": "^1.4.1",
    "gulp-webp": "^4.0.1",
    "postcss": "^8.4.6",
    "tailwindcss": "^2.2.19"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.2.2",
    "@tailwindcss/forms": "^0.3.3"
  }
}
